#1
src_folder = "C:\\Users\\chatchapol.p\\Desktop\\ECA payment 31"
dst_folder = "Z:\\FTPS\\ABIZ\\ECA Payment"
pattern = src_folder + "\ECA payment ABIZ 1-31.xlsx*"
for files in glob.iglob(pattern, recursive=True):
    file_name = os.path.basename(files)
    shutil.copy(files, dst_folder)

